<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<VBox prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Accordion expandedPane="$defaultPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="700.0">
            <panes>
                <TitledPane fx:id="defaultPane" alignment="BOTTOM_LEFT" text="Snippets">
                    <content>
                        <fx:include fx:id="viewSnippets" source="viewSnippets.fxml" />
                    </content>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
                </TitledPane>
                <TitledPane alignment="BOTTOM_LEFT" text="New from template">
                    <content>
                        <fx:include fx:id="viewNew" source="viewNew.fxml" />
                    </content>
                </TitledPane>
            </panes>
        </Accordion>
    </children>
</VBox>
